{
  "name": "remove-github-forks",
  "version": "2.0.1",
  "description": "Delete all forks that have no commits that are not in the main repository",
  "main": "src/index.js",
  "bin": "src/bin.js",
  "scripts": {
    "format": "prettier --write 'src/**/*.{js,md}' *.{js,md}",
    "format-verify": "prettier --check 'src/**/*.{js,md}' *.{js,md}",
    "lint": "eslint src",
    "test": "ava test/*.js",
    "ci": "npm run lint && npm run format-verify && npm run test",
    "prepublishOnly": "npm run ci"
  },
  "keywords": [
    "github",
    "delete",
    "fork"
  ],
  "author": "Denis Sokolov",
  "license": "ISC",
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=6.10"
  },
  "dependencies": {
    "async": "^3.0.1",
    "commander": "^4.0.0",
    "confirm-simple": "^1.0.3",
    "@octokit/rest": "^16.0.1",
    "queue": "^6.0.0",
    "single-line-log": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/denis-sokolov/remove-github-forks.git"
  },
  "bugs": {
    "url": "https://github.com/denis-sokolov/remove-github-forks/issues"
  },
  "homepage": "https://github.com/denis-sokolov/remove-github-forks",
  "devDependencies": {
    "ava": "^2.0.0",
    "eslint": "^6.0.0",
    "prettier": "^1.18.2"
  }
}
